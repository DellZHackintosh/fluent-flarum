    "description": "A Flarum theme comply with Fluent Design System.",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/DellZHackintosh/fluent-flarum/issues",
        "source": "https://github.com/DellZHackintosh/fluent-flarum"
    },
    "require": {
        "flarum/core": "*"
    },
    "extra": {
        "flarum-extension": {
            "title": "Fluent",
            "category": "theme",
            "icon": {
                "name": "fas fa-f",
                "backgroundColor": "#0078d4",
                "color": "#FFF"
            }
        }
    }
